        "class Human:\n",
        "    # コンストラクタ（初期化メソッド）\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    # 情報を出力するメソッド\n",
        "    def printinfo(self):\n",
        "        print(\"名前:\", self.name)\n",
        "        print(\"年齢:\", self.age)\n"
      ]
    }
  ]
}